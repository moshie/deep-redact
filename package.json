{
	"name": "deep-redact",
	"version": "1.0.8",
	"description": "Deeply redact sensitive data from JS primitives",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/moshie/deep-redact.git"
	},
	"scripts": {
		"bench": "pnpm build && node benchmark.mjs",
		"build": "tsup src/index.ts --format esm,cjs --dts",
		"release": "pnpm build && changeset publish",
		"coverage": "vitest run --coverage",
		"test": "vitest run --typecheck",
		"test:watch": "vitest --typecheck",
		"lint": "biome lint --write src/",
		"format": "biome format src/",
		"typecheck": "tsc --noEmit",
		"clean": "rimraf dist"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist/"
	],
	"keywords": [
		"blacklist",
		"sensitive",
		"redaction",
		"redactor",
		"secrets",
		"censor",
		"redact",
		"deep",
		"data",
		"json"
	],
	"bugs": {
		"url": "https://github.com/moshie/deep-redact/issues",
		"email": "hello@moshie.dev"
	},
	"homepage": "https://github.com/moshie/deep-redact",
	"author": {
		"name": "David Hewitt",
		"email": "hello@moshie.dev",
		"url": "https://cv.moshie.dev/"
	},
	"license": "MIT",
	"dependencies": {
		"is-url": "^1.2.4",
		"qs": "^6.14.0",
		"relaxed-json": "^1.0.3"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@changesets/cli": "^2.29.8",
		"@types/benchmark": "^2.1.5",
		"@types/is-url": "^1.2.32",
		"@types/qs": "^6.14.0",
		"@types/relaxed-json": "^1.0.4",
		"@vitest/coverage-v8": "^4.0.16",
		"benchmark": "^2.1.4",
		"lefthook": "^2.0.12",
		"rimraf": "^6.1.2",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"vitest": "^4.0.16"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"lefthook",
			"esbuild"
		]
	},
	"packageManager": "pnpm@10.26.0"
}
